{
  "private": true,
  "name": "default-starter",
  "version": "0.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src/**/*.{js,ts,tsx,graphql} && stylelint src/**/*.{css,scss}",
    "lint:fix": "eslint src/**/*.{js,ts,tsx,graphql} --fix && stylelint src/**/*.{css,scss} --fix",
    "format": "prettier --write  src/**/*.{js,ts,tsx}",
    "prepare": "husky install",
    "analyze": "ANALYZE=1 yarn build",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "serve-storybook": "serve storybook-static"
  },
  "dependencies": {
    "graphql": "^16.5.0",
    "next": "12.2.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@akh-test/eslint-config": "^0.0.5",
    "@akh-test/stylelint-config": "^0.0.2",
    "@babel/core": "^7.18.9",
    "@next/bundle-analyzer": "^12.2.3",
    "@storybook/addon-actions": "^6.4.9",
    "@storybook/addon-essentials": "^6.4.9",
    "@storybook/addon-links": "^6.4.9",
    "@storybook/builder-webpack5": "^6.4.9",
    "@storybook/manager-webpack5": "^6.4.9",
    "@storybook/react": "^6.4.9",
    "@types/node": "18.0.6",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "babel-loader": "^8.2.5",
    "eslint": "8.20.0",
    "husky": "^8.0.1",
    "next-plugin-graphql": "^0.0.2",
    "prettier": "^2.7.1",
    "sass": "^1.49.0",
    "serve": "13.0.2",
    "storybook-addon-next": "1.3.1",
    "stylelint": "^14.9.1",
    "typescript": "4.7.4"
  }
}
